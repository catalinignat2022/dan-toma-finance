# Environment Configuration
NODE_ENV=development
PORT=3001

# API Keys - Get free keys from these providers
# Alpha Vantage: https://www.alphavantage.co/support/#api-key (FREE - 25 requests/day)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Finnhub: https://finnhub.io/register (FREE - 60 requests/minute)
FINNHUB_API_KEY=your_finnhub_key_here

# Polygon.io: https://polygon.io/ (FREE for delayed data)
POLYGON_API_KEY=your_polygon_key_here

# Redis Configuration (for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Database Configuration (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/finviz_clone?schema=public"

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Cache TTL (in seconds)
CACHE_TTL_QUOTES=15
CACHE_TTL_NEWS=300
CACHE_TTL_FUNDAMENTALS=3600
